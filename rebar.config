{plugins, [
  {'lfe-compile', {git, "git://github.com/lfe-rebar3/compile", {tag, "0.5.0"}}}
]}.

{provider_hooks, [{pre, [{compile, {lfe, compile}}]}]}.

{deps, [
  {lfe,       {git, "git://github.com/rvirding/lfe",           {tag, "1.1.1"}}},
  {clj,       {git, "git://github.com/lfex/clj",               {tag, "0.5.0"}}},
  {exemplar,  {git, "git://github.com/lfex/exemplar",          {tag, "0.4.1"}}},
  {levaindoc, {git, "git://github.com/quasiquoting/levaindoc", {tag, "0.5.1"}}},
  {jsx,       {git, "git://github.com/talentdeficit/jsx",      {tag, "2.8.0"}}}
]}.

{overrides, [
  {override, exemplar, [
    {deps, [
      {lutil, {git, "git://github.com/lfex/lutil", {tag, "0.9.0"}}}
    ]}
  ]}
]}.

{profiles, [
  {test, [
    {eunit_compile_opts, [{src_dirs, ["test"]}]},
    {deps, [{ltest, {git, "git://github.com/lfex/ltest", {tag, "0.9.0"}}}]}
  ]}
]}.
